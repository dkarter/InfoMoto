<h1>Listing users</h1>
<center>
  <table>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Email</th>
      <th>Dob</th>
      <th>City</th>
      <th>Street</th>
      <th>State</th>
      <th>Years riding</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user %></td>
      <td><%= user.password %></td>
      <td><%= user.email %></td>
      <td><%= user.dob %></td>
      <td><%= user.city %></td>
      <td><%= user.street %></td>
      <td><%= user.state %></td>
      <td><%= user.years_riding %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
  <% end %>
  </table>
</center>
<br />

<%= link_to 'New User', new_user_path %>
